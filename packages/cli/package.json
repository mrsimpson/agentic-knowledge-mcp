{
  "name": "@codemcp/knowledge-cli",
  "version": "0.1.13",
  "description": "Command-line interface for agentic knowledge web content management",
  "type": "module",
  "main": "dist/exports.js",
  "types": "dist/exports.d.ts",
  "bin": {
    "agentic-knowledge": "./dist/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/exports.js",
      "types": "./dist/exports.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:watch": "tsc -p tsconfig.build.json --watch",
    "clean": "rimraf dist",
    "dev": "tsc -p tsconfig.build.json --watch",
    "lint": "oxlint && eslint .",
    "lint:fix": "oxlint --fix && eslint . --fix",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@codemcp/knowledge-core": "workspace:*",
    "@codemcp/knowledge-content-loader": "workspace:*",
    "@codemcp/knowledge-mcp-server": "workspace:*",
    "commander": "^12.0.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "agentic",
    "knowledge",
    "cli",
    "web-content",
    "documentation",
    "git"
  ],
  "author": "Oliver JÃ¤gle <github@beimir.net>",
  "license": "MIT"
}
